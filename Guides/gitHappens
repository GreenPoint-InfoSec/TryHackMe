+
+    <script>
+      function checkCookie() {
+        if (
+          document.cookie.split(";").some((item) => item.includes("login=1"))
+        ) {
+          console.log('The cookie "login" has "1" for value');
+        } else {
+          window.location.href = "/index.html";
+        }
+      }
+    </script>
+  </body>
+</html>
